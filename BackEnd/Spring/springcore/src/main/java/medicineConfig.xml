	<!-- Configuration for Set -->
	<bean id="medicine"
		class="com.capgemini.springcore.beans.Medicine">
		<property name="name" value="Crocine" />
		<property name="price" value="50.89" />
		<property name="type" value="Allopathic" />
		<property name="drugsSet">
			<set>
				<value>Abc1</value>
				<value>Paracetamol</value>
				<value>Xyz2</value>
				<value>Abc1</value>
			</set>
		</property>
	</bean>

</beans>