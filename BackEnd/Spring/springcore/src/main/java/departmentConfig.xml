<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="dev"
		class="com.capgemini.springcore.beans.DepartmentBean">
		<property name="deptId" value="501" />
		<property name="deptName" value="SD" />
	</bean>

	<bean id="sa"
		class="com.capgemini.springcore.beans.DepartmentBean">
		<property name="deptId" value="502" />
		<property name="deptName" value="SA" />
	</bean>

	<bean id="hr"
		class="com.capgemini.springcore.beans.DepartmentBean"  primary="true">
		<property name="deptId" value="501" />
		<property name="deptName" value="HR" />
	</bean>

</beans>