package com.capgemini.datastructure.algorithm;

public class AlgorithmAnalysis {

	public static void main(String[] args) {
		addUpto(99999999L);
	}
	
	public static long addUpto(Long number) {
		
		long total = 0L;
		
		for(long i = 0; i<= number; i++) {
			total = i + total;
		}
		
		return total;
	}
}
